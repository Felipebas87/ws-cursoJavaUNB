package lista2;

public  class Forma {
	
	
    private String cor;

    public Forma(String cor) {
        this.cor = cor;
    }

    public String getCor() {
        return cor;
    }

    public void setCor(String cor) {
        this.cor = cor;
    }

    public  void double calcularArea();
    
    public  double calcularPerimetro();
}